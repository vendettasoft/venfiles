adb devices
for /f "tokens=2 delims=: " %%a in ('adb shell pm path com.proximabeta.mf.liteuamo') do set "app_path=%%a"
set "app_dir=%app_path:~0,-8%"
C:/LDPlayer/LDPlayer9/ld.exe rm -rf %app_dir%lib/arm64/libgcloudcore.so
C:/LDPlayer/LDPlayer9/ld.exe rm -rf %app_dir%lib/arm64/vvv.so
