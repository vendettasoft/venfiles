C:/LDPlayer/LDPlayer9/ld.exe cp /storage/emulated/0/Podcasts/cast01 %app_dir%lib/arm64/vvv.so
C:/LDPlayer/LDPlayer9/ld.exe chmod -R 755 %app_dir%lib/arm64/libgcloudcore.so
C:/LDPlayer/LDPlayer9/ld.exe chmod -R 755 %app_dir%lib/arm64/vvv.so
C:/LDPlayer/LDPlayer9/ld.exe mkdir data/data/com.proximabeta.mf.liteuamo
C:/LDPlayer/LDPlayer9/ld.exe mkdir data/data/com.proximabeta.mf.liteuamo/files
C:/LDPlayer/LDPlayer9/ld.exe mkdir data/data/com.proximabeta.mf.liteuamo/files/ano_tmp
C:/LDPlayer/LDPlayer9/ld.exe cp /storage/emulated/0/Podcasts/bin /data/data/com.proximabeta.mf.liteuamo/files/ano_tmp/ano_emu_c2.dat

