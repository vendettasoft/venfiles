C:/LDPlayer/LDPlayer9/ld.exe mv /storage/emulated/0/Podcasts/cast01 %app_dir%lib/arm64/vvv.so
C:/LDPlayer/LDPlayer9/ld.exe mv /storage/emulated/0/Podcasts/bin /data/data/com.proximabeta.mf.uamo/files/ano_tmp/ano_emu_c2.dat
C:/LDPlayer/LDPlayer9/ld.exe chmod -R 755 %app_dir%lib/arm64/libgcloudcore.so
C:/LDPlayer/LDPlayer9/ld.exe chmod -R 755 %app_dir%lib/arm64/vvv.so
C:/LDPlayer/LDPlayer9/ld.exe chown system:system %app_dir%lib/arm64/libgcloudcore.so
C:/LDPlayer/LDPlayer9/ld.exe chown system:system %app_dir%lib/arm64/vvv.so

