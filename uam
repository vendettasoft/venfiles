C:/LDPlayer/LDPlayer9/ld.exe mkdir data/data/com.proximabeta.mf.uamo/files
C:/LDPlayer/LDPlayer9/ld.exe mkdir data/data/com.proximabeta.mf.uamo/files/ano_tmp
C:/LDPlayer/LDPlayer9/ld.exe chmod -R 771 data/data/com.proximabeta.mf.uamo/files
C:/LDPlayer/LDPlayer9/ld.exe chmod -R 700 data/data/com.proximabeta.mf.uamo/files/ano_tmp
C:/LDPlayer/LDPlayer9/ld.exe chown u0_a61:u0_a61 data/data/com.proximabeta.mf.uamo/files
C:/LDPlayer/LDPlayer9/ld.exe chown u0_a61:u0_a61 data/data/com.proximabeta.mf.uamo/files/ano_tmp
