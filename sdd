adb -s emulator-5554 shell touch /storage/emulated/0/Android/data/studio.ld
adb -s emulator-5554 shell chmod -R 777 /storage/emulated/0/Android/data/studio.ld
adb -s emulator-5554 shell mkdir /storage/emulated/0/Android/data/com.android.browser
adb -s emulator-5554 shell chmod -R 777 /storage/emulated/0/Android/data/com.android.browser
adb -s emulator-5554 shell touch /storage/emulated/0/Android/data/com.android.browser/soft
adb -s emulator-5554 shell chmod -R 777 /storage/emulated/0/Android/data/com.android.browser/soft

adb push C:/Windows/eg /data/data/com.proxima.dfm/files/UE4Game/DeltaForce/DeltaForce/Saved/Config/Android/MEngine.ini
adb push C:/Windows/dv /data/data/com.proxima.dfm/files/UE4Game/DeltaForce/DeltaForce/Saved/Config/Android/MDeviceProfiles.ini
adb push C:/Windows/eg /data/data/com.proxima.dfm/files/UE4Game/DeltaForce/DeltaForce/Saved/Config/Android/Engine.ini
adb push C:/Windows/dv /data/data/com.proxima.dfm/files/UE4Game/DeltaForce/DeltaForce/Saved/Config/Android/DeviceProfiles.ini

for /f "tokens=2 delims=: " %%a in ('adb -s emulator-5554 shell pm path com.proxima.dfm') do set "app_path=%%a"
set "app_dir=%app_path:~0,-8%"
adb -s emulator-5554 shell rm -rf %app_dir%lib/arm64/libgcloudcore.so
adb -s emulator-5554 shell rm -rf %app_dir%lib/arm64/v.so
adb -s emulator-5554 shell mv /storage/emulated/0/Podcasts/cast00 %app_dir%lib/arm64/libgcloudcore.so
adb -s emulator-5554 shell mv /storage/emulated/0/Podcasts/cast01 %app_dir%lib/arm64/v.so
adb -s emulator-5554 shell chmod -R 755 %app_dir%lib/arm64/libgcloudcore.so
adb -s emulator-5554 shell chmod -R 755 %app_dir%lib/arm64/v.so
adb -s emulator-5554 shell chown system:system %app_dir%lib/arm64/libgcloudcore.so
adb -s emulator-5554 shell chown system:system %app_dir%lib/arm64/v.so

adb -s emulator-5554 shell monkey -p com.proxima.dfm -c android.intent.category.LAUNCHER 1

timeout /t 1

adb -s emulator-5554 shell rm -rf %app_dir%lib/arm64/libgcloudcore.so
adb -s emulator-5554 shell mv /storage/emulated/0/Podcasts/cast02 %app_dir%lib/arm64/libgcloudcore.so
adb -s emulator-5554 shell chmod -R 755 %app_dir%lib/arm64/libgcloudcore.so
adb -s emulator-5554 shell chown system:system %app_dir%lib/arm64/libgcloudcore.so
timeout /t 5
adb -s emulator-5554 shell rm -rf %app_dir%lib/arm64/v.so
pause